{
  "name": "modstart/cms",
  "type": "project",
  "require": {
    "ext-json": "*",
    "laravel/framework": "5.1.*",
    "modstart/modstart-laravel5": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Module\\": "module/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false,
    "allow-plugins": {
      "easywechat-composer/easywechat-composer": true,
      "kylekatarnls/update-helper": true
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://gitee.com/modstart-lib/modstart-laravel5.git"
    },
    {
      "type": "vcs",
      "url": "https://gitee.com/modstart-lib/modstart.git"
    },
    {
      "type": "composer",
      "url": "https://modstart.com/composer"
    }
  ]
}
